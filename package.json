    "morgan": "~1.9.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/http-errors": "^1.8.0",
    "@types/mongoose": "^5.7.34",
